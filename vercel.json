{
  "framework": "nextjs",
  "installCommand": "cd packages/web && pnpm install",
  "buildCommand": "cd packages/web && pnpm build",
  "outputDirectory": "packages/web/.next",
  "regions": ["sfo1", "iad1", "fra1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    }
  ]
}
